<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <Spinner
        android:id="@+id/SlamInitType"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="invisible"></Spinner>

    <Spinner
        android:id="@+id/SlamMode"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:visibility="invisible"></Spinner>

    <Spinner
        android:id="@+id/spinner_2d_dbs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></Spinner>
 <Spinner
        android:id="@+id/spinner_dat_points"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></Spinner>

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/ll_edge_tracker"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <Spinner
            android:id="@+id/spinner_cao"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </Spinner>

        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="设置参数"
            android:onClick="go2SettingParams"
            />

    </LinearLayout>


    <Button
        android:id="@+id/camera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:gravity="center"
        android:onClick="CameraSource"
        android:text="Camera"
        android:textSize="22sp"
        android:visibility="invisible"/>
</LinearLayout>